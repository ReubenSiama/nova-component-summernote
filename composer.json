{
    "name": "cimpleo/nova-summernote",
    "description": "A Laravel Nova wrapper for the Summernote WYSIWYG editor",
    "keywords": [
        "laravel",
        "nova",
        "wysiwyg",
        "summernote"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Cimpleo\\NovaSummernote\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cimpleo\\NovaSummernote\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
